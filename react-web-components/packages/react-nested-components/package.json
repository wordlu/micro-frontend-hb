        "react-scripts": "3.4.1"
    },
    "devDependencies": {
        "eslint-plugin-node": "^11.0.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "^7.18.0",
        "to-string-loader": "^1.1.6",
        "jest-environment-jsdom-fourteen": "0.1.0",
        "react-app-rewired": "2.1.3",
        "react-test-renderer": "16.9.0",
        "webpack-cli": "^3.3.11"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
